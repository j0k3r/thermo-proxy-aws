{
    "name": "j0k3r/thermo-proxy-aws",
    "description": "A thermo proxy running on AWS",
    "license": "MIT",
    "require": {
        "bref/bref": "^0.5.3",
        "bref/dynamap": "^0.2.0",
        "bref/logger": "^1.0",
        "influxdb/influxdb-php": "^1.15",
        "nyholm/psr7": "^1.1",
        "nyholm/psr7-server": "^1.0.0",
        "php-di/php-di": "^6.0",
        "slim/slim": "^4.0",
        "symfony/yaml": "^5.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-deprecation-rules": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "symfony/phpunit-bridge": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Thermo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Thermo\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.2.20"
        },
        "sort-packages": true
    }
}
