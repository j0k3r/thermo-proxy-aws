{
    "name": "j0k3r/thermo-proxy-aws",
    "description": "A thermo proxy running on AWS",
    "license": "MIT",
    "require": {
        "bref/bref": "^1.0.0",
        "bref/dynamap": "^0.3.0",
        "bref/logger": "^1.0",
        "influxdb/influxdb-php": "^1.15",
        "nyholm/psr7": "^1.1",
        "nyholm/psr7-server": "^1.0.0",
        "php-di/php-di": "^6.0",
        "slim/slim": "^4.0",
        "symfony/polyfill-php80": "^1.27",
        "symfony/yaml": "^5.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "symfony/phpunit-bridge": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Thermo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Thermo\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4.10"
        },
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
